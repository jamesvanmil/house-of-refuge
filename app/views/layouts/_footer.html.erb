          <%= link_to "#", id: "envelope-icon" do %>
            <i class="fa fa-3x fa-envelope fa-fw"></i>
          <% end %>
          <%= link_to "http://www.github.com/jamesvanmil/house-of-refuge", target: "_blank", id: "git-icon" do %>
